# Environment Variables Example
# Copy this file to .env and fill in your actual values

# Google Analytics 4 Tracking ID
VITE_GA_TRACKING_ID=G-XXXXXXXXXX

# SiteGround Affiliate Configuration
VITE_SITEGROUND_AFFILIATE_ID=your-affiliate-id
VITE_SITEGROUND_BASE_URL=https://www.siteground.com/go/your-affiliate-id

# Site Configuration
VITE_SITE_URL=https://yourdomain.com
VITE_SITE_NAME=SiteGround Hosting Promotion

# API Keys (if needed)
VITE_GEOLOCATION_API_KEY=your-geolocation-api-key

# Development Settings
VITE_DEV_MODE=true
VITE_DEBUG_SEO=false
